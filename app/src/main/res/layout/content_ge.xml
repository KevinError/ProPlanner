<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.planner.planner.GEActivity"
    tools:showIn="@layout/activity_ge">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <CheckBox
                android:id="@+id/a1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="selectItem"
                android:text="A1"/>

            <CheckBox
                android:id="@+id/a2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="selectItem"
                android:text="A2" />

            <CheckBox
                android:id="@+id/a3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="selectItem"
                android:text="A3"/>

            <CheckBox
                android:id="@+id/B1a"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="selectItem"
                android:text="B1a"/>

            <CheckBox
                android:id="@+id/B1b"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="selectItem"
                android:text="B1b"/>

            <CheckBox
                android:id="@+id/B2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="selectItem"
                android:text="B2"/>

            <CheckBox
                android:id="@+id/C1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="selectItem"
                android:text="C1"/>

            <CheckBox
                android:id="@+id/C2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="selectItem"
                android:text="C2" />

            <CheckBox
                android:id="@+id/C3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="selectItem"
                android:text="C3"/>

            <CheckBox
                android:id="@+id/D1a"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="selectItem"
                android:text="D1a"/>

            <CheckBox
                android:id="@+id/D1b"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="selectItem"
                android:text="D1b"/>

            <CheckBox
                android:id="@+id/D2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="selectItem"
                android:text="D2"/>

            <CheckBox
                android:id="@+id/E"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="selectItem"
                android:text="E"/>

            <CheckBox
                android:id="@+id/F1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="selectItem"
                android:text="F1"/>

            <CheckBox
                android:id="@+id/F2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="selectItem"
                android:text="F2"/>

            <CheckBox
                android:id="@+id/F3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="selectItem"
                android:text="F3"/>

            <CheckBox
                android:id="@+id/GlobalIssue"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="selectItem"
                android:text="Global Issuse"/>

            <CheckBox
                android:id="@+id/HumanDiversity"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="selectItem"
                android:text="HumanDiversity"/>


            <Button
                android:id="@+id/confirm"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:onClick="confirm"
                android:text="Confirm" />

        </LinearLayout>
    </ScrollView>

</android.support.constraint.ConstraintLayout>
